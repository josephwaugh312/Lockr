# Production Environment Variables for Custom Domain
# Frontend (www.lockrr.app)
NEXT_PUBLIC_API_BASE_URL=https://api.lockrr.app/api/v1
NEXT_PUBLIC_APP_URL=https://www.lockrr.app

# Backend (api.lockrr.app) 
ALLOWED_ORIGINS=https://www.lockrr.app,https://lockrr.app
FRONTEND_URL=https://www.lockrr.app
CORS_ORIGIN=https://www.lockrr.app

# Security Headers
HSTS_MAX_AGE=31536000
SECURE_COOKIES=true

# Domain-specific settings
APP_DOMAIN=lockrr.app
API_DOMAIN=api.lockrr.app 