{
  "name": "lockr-backend",
  "version": "0.1.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "node server.js",
    "server": "node server.js",
    "start:backend": "node scripts/encrypt-production-data.js ; node server-simple.js",
    "migrate": "node migrations/run.js",
    "lint": "next lint",
    "test": "NODE_ENV=test jest --coverage",
    "test:react": "NODE_ENV=test jest --config jest.config.react.js",
    "test:integration": "NODE_ENV=test jest tests/integration --runInBand",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:ci": "npm run test && npm run test:react",
    "test:diff-coverage": "node scripts/check-diff-coverage.js",
    "test:integration:coverage": "jest tests/integration --coverage --maxWorkers=1",
    "test:full:isolated": "npm run test:integration && jest --testPathIgnorePatterns=tests/integration",
    "test:unit": "jest tests/controllers tests/services tests/middleware tests/utils",
    "test:all": "jest --coverage",
    "test:ci": "jest --coverage --ci --maxWorkers=2",
    "test:backend": "jest --config jest.config.backend.js",
    "test:safe": "jest --config jest.config.backend.js",
    "test:frontend": "jest --testMatch='**/*.test.tsx' --forceExit --testTimeout=10000",
    "test:working": "jest tests/server.test.js tests/utils tests/middleware tests/models --forceExit --testTimeout=5000",
    "test:db:init": "node scripts/initializeTestDatabase.js",
    "test:db:check": "node scripts/testDatabaseConnection.js",
    "test:db:setup": "npm run test:db:init && npm run test:db:check"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-query-devtools": "^5.80.6",
    "argon2": "^0.43.0",
    "axios": "^1.9.0",
    "clsx": "^2.0.0",
    "colors": "^1.4.0",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "framer-motion": "^11.18.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.525.0",
    "next": "^15.3.3",
    "next-themes": "^0.2.1",
    "node-cron": "^4.1.0",
    "pg": "^8.16.0",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "resend": "^4.5.2",
    "sonner": "^2.0.5",
    "speakeasy": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "twilio": "^5.7.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.28.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "18.3.23",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.0.1",
    "babel-jest": "^30.0.0-beta.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "postcss": "^8",
    "sharp": "^0.34.3",
    "supertest": "^7.1.1",
    "tailwindcss": "^3.3.0",
    "typescript": "5.8.3"
  }
}
